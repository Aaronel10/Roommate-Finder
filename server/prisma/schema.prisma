generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id                  Int     @id @default(autoincrement())
  question_text       String
  response_type       String
  question_text_short String
  is_filter           Boolean
}

model User {
  id            Int      @id @default(autoincrement())
  email         String
  hash_password String
  is_active     Boolean
  is_superuser  Boolean
  is_verified   Boolean
  image         String
  gender        String
  first_name    String
  last_name     String
  birthday      DateTime
  phone_number  Int
  zip_code      Int
  city          String
  state         String
  reset_id      Int
  reset_exp     DateTime
  update_id     Int
  update_exp    DateTime
  activate_id   Int
  activate_exp  DateTime
  is_setup      Boolean
  setup_step    String
}
